{
    "content_scripts": [
        {
            "matches": [
                "http://*/*", 
                "https://*/*"
            ], 
            "all_frames": true, 
            "run_at": "document_start", 
            "js": [
                "includes/content.js"
            ]
        }
    ], 
    "name": "VersionOneEnhancements", 
    "icons": {
        "128": "icons/icon128.png", 
        "48": "icons/icon48.png", 
        "16": "icons/button.png"
    }, 
    "web_accessible_resources": [
        "*"
    ], 
    "description": "The extension that reduces your pain from using VersionOne and makes you happy.", 
    "background": {
        "page": "background.html"
    }, 
    "homepage_url": "https://github.com/snake-345/VersionOneEnhancements", 
    "version": "1.2.2", 
    "options_page": "options.html", 
    "manifest_version": 2, 
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'", 
    "browser_action": {
        "default_icon": "icons/button.png", 
        "default_title": "Click for go to options"
    }, 
    "permissions": [
        "tabs", 
        "http://*/*", 
        "https://*/*", 
        "contextMenus", 
        "webNavigation"
    ]
}